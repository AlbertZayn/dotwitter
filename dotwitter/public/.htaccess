RewriteEngine On
RewriteCond %{REQUEST_URI} !\.(png|jpg|gif)$
RewriteBase /
RewriteCond %{REQUEST_FILENAME} !-f
RewriteCond %{REQUEST_FILENAME} !-d
RewriteRule ^(.*)$ index.php?q=$1 [L,QSA]

